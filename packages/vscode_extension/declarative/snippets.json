{
  "Function": {
    "prefix": "fun",
    "body": ["fun ${1:name}(${2:parameters}): ${3:returnType} {", "\t$0", "}"]
  },
  "Impl": {
    "prefix": "impl",
    "body": ["impl ${1:type} {", "\t$0", "}"]
  },
  "Main function without arguments": {
    "prefix": "main",
    "body": ["fun main() {", "\t$0", "}"]
  },
  "Main function with arguments": {
    "prefix": "maina",
    "body": ["fun main(List<String> args) {", "\t$0", "}"]
  },
  "Trait": {
    "prefix": "trait",
    "body": ["trait ${1:name} {", "\t$0", "}"]
  },
  "Type": {
    "prefix": "type",
    "body": ["type ${1:name} = $0"]
  }
}
