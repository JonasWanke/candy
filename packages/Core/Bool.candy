function = use "..Function"
type = use "..Type"

is value := type.is value Bool

not a :=
  needs (is a)
  ✨.ifElse a { False } { True }
and a b :=
  needs (is a)
  needs (is b)
  ✨.ifElse a { b } { False }
or a b :=
  needs (is a)
  needs (is b)
  ✨.ifElse a { True } { b }
implies a b :=
  needs (is a)
  needs (is b)
  ✨.ifElse a { b } { True }
